        <definition>SimpleTrack desmoj.extensions.space3D.SpatialLayoutManager.getTrackStraightTo</definition>
        <argsstring>(MovableSpatialObject mover, String exitPoint, SpatialObject destination, String entryPoint)</argsstring>
        <name>getTrackStraightTo</name>
        <param>
          <type><ref refid="interfacedesmoj_1_1extensions_1_1space3_d_1_1_movable_spatial_object" kindref="compound">MovableSpatialObject</ref></type>
