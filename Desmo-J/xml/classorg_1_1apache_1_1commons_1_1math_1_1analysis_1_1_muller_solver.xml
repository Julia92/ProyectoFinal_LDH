      <memberdef kind="function" id="classorg_1_1apache_1_1commons_1_1math_1_1analysis_1_1_muller_solver_1a930871c7344d44e4abf11108461d1f80" prot="public" static="no" const="no" explicit="no" inline="yes" virt="non-virtual">
        <type>double</type>
        <definition>double org.apache.commons.math.analysis.MullerSolver.solve2</definition>
        <argsstring>(double min, double max)</argsstring>
        <name>solve2</name>
        <param>
          <type>double</type>
          <declname>min</declname>
        </param>
